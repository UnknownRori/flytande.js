{
    "name": "@unknownrori/flytande.js",
    "version": "0.0.0",
    "description": "Simple and yet fluent general purpose query builder for sql language",
    "files": [
        "dist",
        "package.json"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "./dist/index.js",
