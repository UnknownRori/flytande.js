    "bugs": {
        "url": "https://github.com/UnknownRori/flytande.js/issues"
    },
    "homepage": "https://github.com/UnknownRori/flytande.js#readme",
    "devDependencies": {
        "@types/node": "^20.10.5",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "eslint": "^8.56.0",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
